package com.example.demo.domain;

public class PagingVO {
	
	private int pagNo;
	private int qty;
	
	private String type;
	private String keyword;
	
	public PagingVO(){
		this.pagNo=1;
		this.qty=10;
	}
	
	public PagingVO(int pageNo, int qty) {
		this.pagNo = pageNo;
		this.qty=qty;
	}
	
	public int getPageStart() {
		return (pagNo-1)*qty; // DBÏùò limit #{pageStart}
	}
	
}
